<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="759.0" prefWidth="915.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.AdditionWord">
    <TextArea fx:id="wordMean" layoutX="362.0" layoutY="105.0" prefHeight="577.0" prefWidth="488.0"/>
    <Label layoutX="565.0" layoutY="33.0" prefHeight="50.0" prefWidth="83.0" text="Mean">
        <font>
            <Font size="32.0"/>
        </font>
    </Label>
    <Label layoutX="131.0" layoutY="105.0" prefHeight="17.0" prefWidth="57.0" text="Word">
        <font>
            <Font size="23.0"/>
        </font>
    </Label>
    <TextField fx:id="wordAdd" layoutX="84.0" layoutY="159.0" prefHeight="44.0" prefWidth="149.0"/>
    <Label layoutX="110.0" layoutY="224.0" prefHeight="34.0" prefWidth="100.0" text="Type Word">
        <font>
            <Font size="20.0"/>
        </font>
    </Label>
    <Label layoutX="123.0" layoutY="350.0" prefHeight="28.0" prefWidth="71.0" text="Spelling">
        <font>
            <Font size="20.0"/>
        </font>
    </Label>
    <TextField fx:id="wordType" layoutX="86.0" layoutY="280.0" prefHeight="44.0" prefWidth="149.0"/>
    <TextField fx:id="wordSpelling" layoutX="86.0" layoutY="401.0" prefHeight="44.0" prefWidth="149.0"/>
    <Button fx:id="save" layoutX="796.0" layoutY="19.0" mnemonicParsing="false" onAction="#saveWord" text="Save"/>
    <Button fx:id="backHome" layoutX="850.0" layoutY="19.0" mnemonicParsing="false" onAction="#backToHome"
            text="Back"/>
    <Label layoutX="31.0" layoutY="682.0" prefHeight="30.0" prefWidth="331.0"
           text="Note : fill in the information completely or the word not save !!"/>
</AnchorPane>
